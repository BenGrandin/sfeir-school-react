{
  "name": "sfeirschoolreact",
  "version": "1.0.0",
  "description": "Sfeir School React with revealJS",
  "main": "index.js",
  "scripts": {
    "sass": "node-sass scss/react.scss -o ./css/ --source-map=true --watch=\\\"scss\\\" --recursive=\\\"scss\\\"",
    "sass-once": "node-sass scss/react.scss -o ./css/ --source-map=true ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "live-server --port=4242 --open=\"./index.html\" --watch=\"markdown/\"",
    "start": "parallelshell \"npm run serve\" \"npm run sass\"",
    "update-theme": "rm -rf ./sfeir-school-theme ./_tmp && git clone git@github.com:sfeir-open-source/sfeir-school-theme.git ./_tmp && cp -R ./_tmp/dist sfeir-school-theme && rm -rf ./_tmp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jefBinomed/SfeirSchoolTheme.git"
  },
  "keywords": [
    "react",
    "redux",
    "sfeir"
  ],
  "author": "wgoedel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jefBinomed/SfeirSchoolTheme/issues"
  },
  "homepage": "https://github.com/jefBinomed/SfeirSchoolTheme#readme",
  "dependencies": {
    "chokidar-cli": "^1.2.2",
    "cpx": "^1.5.0",
    "highlight.js": "^9.15.6",
    "live-server": "^1.2.1",
    "node-sass": "^4.12.0",
    "parallelshell": "3.0.1",
    "prettier": "^2.0.2",
    "reveal.js": "^3.8.0",
    "sfeir-school-theme": "^1.0.0"
  }
}
